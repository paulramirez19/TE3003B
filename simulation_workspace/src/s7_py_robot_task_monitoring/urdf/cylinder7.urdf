<?xml version="1.0"?>
<robot name="my_robot">
    <link name="cylinder7/base_footprint"/>

    <link name="cylinder7/base_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.0"/>
            <geometry>
                <cylinder radius="0.125" length="0.01"/>
            </geometry>
            <material name="electric_purple">
                <color rgba="0.6 0.2 1.0 1.0" />
            </material>
        </visual>
    </link>

    <joint name="base_joint" type="fixed">
        <origin xyz="0.0 0.0 -0.005" rpy="0.0 0.0 0.0"/>
        <parent link="cylinder7/base_footprint"/>
        <child link="cylinder7/base_link"/>
    </joint>
</robot>
